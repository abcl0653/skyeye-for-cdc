<core:FragmentDefinition xmlns="sap.m" 
	xmlns:f="sap.f" 
	xmlns:core="sap.ui.core">
	<ResponsivePopover placement="Bottom" showHeader="false">
		<f:ProductSwitch change="onProductChanged" items="{pageConfig>/}">
			<f:items>
				<f:ProductSwitchItem src="{pageConfig>Icon}" title="{pageConfig>Title}" subTitle="{pageConfig>Subtitle}" target="{pageConfig>Id}" />
			</f:items>
		</f:ProductSwitch>
	</ResponsivePopover>
</core:FragmentDefinition>