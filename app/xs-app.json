{
    "welcomeFile": "webapp/",
    "authenticationMethod": "route",
    "logout": {
      "logoutEndpoint": "/do/logout"
    },
    "routes": [{
        "source": "^/webapp/(.*)$",
        "target": "$1",
        "localDir": "webapp/"
    },
    {
        "source": "^(.*)$",
        "authenticationType": "xsuaa",
        "csrfProtection": true,
        "destination": "srv-binding"
    }]
}