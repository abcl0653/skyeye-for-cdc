{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ibso.skyeyeForCdc",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sap.ibso.skyeyeForCdc.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.tnt": {},
				"sap.suite.ui.commons":{}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ibso.skyeyeForCdc.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sap.ibso.skyeyeForCdc.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"Error"
					]
				}
			},
			"routes": [{
				"name": "Error",
				"pattern": "error",
				"target": ["Error"]
			},{
				"name": "Home",
				"pattern": "home",
				"target": ["Home"]
			},{
				"name": "Monitoring",
				"pattern": "monitoring",
				"target": ["Monitoring"]
			},{
				"name": "Case",
				"pattern": "case/list",
				"target": ["Case"]
			},{
				"name": "CaseDetail",
				"pattern": "case/detail",
				"target": ["CaseDetail"]
			},{
				"name": "Block",
				"pattern": "block/list",
				"target": ["Block"]
			},{
				"name": "BlockDetail",
				"pattern": "block/detail",
				"target": ["BlockDetail"]
			},{
				"name": "Task",
				"pattern": "task/list",
				"target": ["Task"]
			},{
				"name": "TaskDetail",
				"pattern": "task/detail",
				"target": ["TaskDetail"]
			},{
				"name": "Staff",
				"pattern": "staff/list",
				"target": ["Staff"]
			},{
				"name": "StaffDetail",
				"pattern": "staff/detail",
				"target": ["StaffDetail"]
			},{
				"name": "Region",
				"pattern": "region",
				"target": ["Region"]
			},{
				"name": "Supply",
				"pattern": "supply",
				"target": ["Supply"]
			},{
				"name": "MobileUpload",
				"pattern": "mobile",
				"target": ["MobileUpload"]
			}],
			"targets": {
				"Home": {
					"viewType": "XML",
					"viewName": "Home",
					"title":"Home"
				},
				"Error": {
					"viewType": "XML",
					"viewName": "Error",
					"title":"Error"
				},
				"Monitoring": {
					"viewType": "XML",
					"viewName": "Monitoring",
					"title":"Monitoring"
				},
				"Case": {
					"viewType": "XML",
					"viewName": "Case",
					"title":"Case"
				},
				"CaseDetail": {
					"viewType": "XML",
					"viewName": "CaseDetail",
					"title":"Case"
				},
				"Block": {
					"viewType": "XML",
					"viewName": "Block",
					"title":"Block"
				},
				"BlockDetail": {
					"viewType": "XML",
					"viewName": "BlockDetail",
					"title":"Block"
				},
				"Task": {
					"viewType": "XML",
					"viewName": "Task",
					"title":"Task"
				},
				"TaskDetail": {
					"viewType": "XML",
					"viewName": "TaskDetail",
					"title":"Task"
				},
				"Staff": {
					"viewType": "XML",
					"viewName": "Staff",
					"title":"Staff"
				},
				"StaffDetail": {
					"viewType": "XML",
					"viewName": "StaffDetail",
					"title":"Staff"
				},
				"Region": {
					"viewType": "XML",
					"viewName": "Region",
					"title":"Region"
				},
				"Supply": {
					"viewType": "XML",
					"viewName": "Supply",
					"title":"Supply"
				},
				"MobileUpload": {
					"viewType": "XML",
					"viewName": "MobileUpload",
					"title":"MobileUpload"
				}
			}
		}
	}
}