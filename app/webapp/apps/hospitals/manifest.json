{
  "_version": "1.8.0",
  "sap.app": {
    "id": "hospitals",
    "type": "application",
    "title": "Browse Hospitals",
    "description": "Sample Application - Hospitals",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "Hospital": {
        "uri": "/facility/hospital/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "dependencies": {
      "libs": {
        "sap.fe": {
          
        }
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "Hospital",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "HospitalsList",
          "target": "HospitalsList"
        },
        {
          "pattern": "Hospitals({key}):?query:",
          "name": "HospitalsDetails",
          "target": "HospitalsDetails"
        }
      ],
      "targets": {
        "HospitalsList": {
          "type": "Component",
          "id": "HospitalsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Hospitals",
              "navigation": {
                "Hospitals": {
                  "detail": {
                    "route": "HospitalsDetails"
                  }
                }
              }
            }
          }
        },
        "HospitalsDetails": {
          "type": "Component",
          "id": "HospitalsDetailsList",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Hospitals"
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  }
}